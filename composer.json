{
    "name": "sven/artisan-shortcuts",
    "description": "Register shortcuts for executing artisan commands.",
    "keywords": ["laravel", "artisan", "php", "laravel-artisan", "shortcuts", "shorthands", "commands", "cli"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "svenluijten1995@gmail.com",
            "homepage": "https://svenluijten.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/console": "5.5.* || 5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Sven\\ArtisanShortcuts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sven\\ArtisanShortcuts\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sven\\ArtisanShortcuts\\ServiceProvider"
            ]
        }
    }
}
