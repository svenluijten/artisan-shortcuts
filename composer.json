{
    "name": "sven/artisan-shortcuts",
    "description": "Register shortcuts for executing artisan commands.",
    "keywords": [
        "laravel",
        "artisan",
        "php",
        "laravel-artisan",
        "shortcuts",
        "shorthands",
        "commands",
        "cli"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "contact@svenluijten.com",
            "homepage": "https://svenluijten.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/console": "^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0 || ^5.0",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Sven\\ArtisanShortcuts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sven\\ArtisanShortcuts\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sven\\ArtisanShortcuts\\ServiceProvider"
            ]
        }
    }
}
